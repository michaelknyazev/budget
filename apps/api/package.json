{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "node dist/main",
    "start:prod": "node dist/main",
    "lint": "eslint src/",
    "check-types": "tsc --noEmit",
    "test": "jest",
    "infra:up": "docker compose up -d",
    "infra:down": "docker compose down",
    "infra:restart": "docker compose restart",
    "infra:reset": "docker compose down -v && docker compose up -d && sleep 2 && pnpm db:migrate",
    "db:migrate": "mikro-orm migration:up",
    "db:migrate:create": "mikro-orm migration:create",
    "db:migrate:fresh": "mikro-orm migration:fresh",
    "db:seed": "ts-node -r tsconfig-paths/register src/database/seed.ts",
    "repl": "nest start --entryFile repl",
    "repl:ts": "ts-node -r tsconfig-paths/register src/repl-ts.ts"
  },
  "dependencies": {
    "@budget/schemas": "workspace:*",
    "@mikro-orm/cli": "^6",
    "@mikro-orm/core": "^6",
    "@mikro-orm/migrations": "^6",
    "@mikro-orm/nestjs": "^6",
    "@mikro-orm/postgresql": "^6",
    "@mikro-orm/reflection": "^6",
    "@nestjs/common": "^10",
    "@nestjs/config": "^3",
    "@nestjs/core": "^10",
    "@nestjs/platform-express": "^10",
    "@nestjs/swagger": "^8",
    "@nestjs/throttler": "^6",
    "better-auth": "^1",
    "nestjs-pino": "^4",
    "nestjs-zod": "^4",
    "pg": "^8.18.0",
    "pino-http": "^10",
    "pino-pretty": "^13",
    "reflect-metadata": "^0.2",
    "rxjs": "^7",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "zod": "^3.24"
  },
  "devDependencies": {
    "@nestjs/cli": "^10",
    "@nestjs/schematics": "^10",
    "@nestjs/testing": "^10",
    "@types/express": "^5",
    "@types/multer": "^2.0.0",
    "@types/node": "^22",
    "@types/pg": "^8.16.0",
    "ts-node": "^10",
    "tsconfig-paths": "^4",
    "typescript": "^5.7"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./mikro-orm.config.ts"
    ]
  }
}
